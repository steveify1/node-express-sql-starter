{
  "name": "crypt-invest",
  "version": "1.0.0",
  "description": "A multi-currency crypto investment API",
  "main": "src/index.js",
  "scripts": {
    "babel-node": "babel-node --max_old_space_size=5048 src/index.js",
    "dev": "nodemon --exec npm run babel-node",
    "clean": "if exist dist (rm -r dist)",
    "build": "babel src --out-dir dist",
    "build2": "babel src --out-dir dist && npm run db-reset:prod",
    "start": "babel-node  src/index.js",
    "test": "npm run db-migrate:test && npm run db-seeder:test && nyc --reporter=html --reporter=text mocha --recursive src/tests/ --require @babel/register --timeout 60000 --exit",
    "test:dev": "SET NODE_ENV=test && npm run test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test:coverage": "npm test && npm run coveralls",
    "lint": "eslint src/**/*.js",
    "db-migrate": "sequelize db:migrate --env production",
    "db-migrate:dev": "sequelize db:migrate --env development",
    "db-seeder:dev": "sequelize db:seed:all --env development",
    "db-seeder": "sequelize db:seed:all --env production",
    "db-seeder:test": "sequelize db:seed:all --env test",
    "db-delete:dev": "sequelize db:migrate:undo:all --env development",
    "db-delete": "sequelize db:migrate:undo:all --env production",
    "db-migrate:test": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test",
    "db-migrate-seed:dev": "npm run db-migrate:dev && npm run db-seeder:dev",
    "db-migrate-seed": "npm run db-migrate && npm run db-seeder",
    "db-reset:dev": "npm run db-delete:dev && npm run db-migrate-seed:dev",
    "db-reset:prod": "npm run db-delete && npm run db-migrate-seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steveify1/crypt-invest.git"
  },
  "keywords": [],
  "author": "Nwakasi Stephen Ifeanyi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/steveify1/crypt-invest/issues"
  },
  "homepage": "https://github.com/steveify1/crypt-invest#readme",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.8.6",
    "@babel/runtime": "^7.8.7",
    "axios": "^0.21.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^6.0.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-node": "^4.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.3",
    "handlebars": "^4.7.6",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^7.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "moment": "^2.29.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.4.16",
    "nyc": "^15.0.0",
    "pg": "^7.18.2",
    "query-string": "^6.13.1",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "sinon": "^9.0.1",
    "supertest": "^4.0.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/compat-data": "^7.10.4",
    "coveralls": "^3.1.0"
  },
  "engines": {
    "node": "12"
  }
}
